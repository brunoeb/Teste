unit Util.Util;

interface

uses Vcl.ComCtrls;

function Ifthen(Condicao:boolean;ValorTrue,ValorFalse:Variant):variant;


implementation

function Ifthen(Condicao:boolean;ValorTrue,ValorFalse:Variant):variant;
begin
  if Condicao then
    Result := ValorTrue
  else
    Result := ValorFalse;
end;

